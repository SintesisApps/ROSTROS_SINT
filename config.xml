<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.cenet.hotel"
        version   = "0.0.1">
    <name>Rostros Sint</name>

    <description>
        rostros
    </description>


    <feature name="http://api.phonegap.com/1.0/device" />
	<feature name="http://api.phonegap.com/1.0/camera" />

    <preference name="phonegap-version" value="2.7.0" />
    <preference name="orientation"      value="default" />
    <preference name="target-device"    value="universal" />
    <preference name="fullscreen"       value="true" />

    <icon src="img/logo.jpg" />
    <icon src="img/logo.jpg"   gap:platform="android"    gap:density="ldpi" />
    <icon src="img/logo.jpg"   gap:platform="ios"        width="57" height="57" />

	
	<access origin="*" subdomains="true" />
</widget>